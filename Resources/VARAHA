<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script>
  // Use jQuery to load content from file
$(document).ready(function(){
  $.get('TEGRAT9', function(data) {
    $('#TheListOfNodes').html('<pre>' + data + '</pre>');
  }).fail(function() {
    $('#TheListOfNodes').text('Error Loading...');
  });
}); 
  document.getElementById('toggleButton').addEventListener('click', function() {
    var content = document.getElementById('TheListOfNodes');
    if (content.style.display === 'none') {
      content.style.display = 'block';
      this.textContent = '-'; // Change to minus when expanded
    } else {
      content.style.display = 'none';
      this.textContent = '+'; // Change to plus when collapsed
    }
  }); 
  document.getElementById('toggleButton2').addEventListener('click', function() {
    var content = document.getElementById('TheListOfApps');
    if (content.style.display === 'none') {
      content.style.display = 'block';
      this.textContent = '-'; // Change to minus when expanded
    } else {
      content.style.display = 'none';
      this.textContent = '+'; // Change to plus when collapsed
    }
  });  
</script>
